{
  "Urls": "http://*:80",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "LogFile": "/var/log/nutrition-tracker/log.txt"
  },
  "CancellationToken": {
    "Delay": 5000
  }
}
