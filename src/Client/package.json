{
  "name": "nutrition-tracker-client",
  "private": true,
  "version": "0.0.1",
  "author": {
    "name": "Vidar André Bø"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf --glob ../Server/wwwroot/* && tsc && vite build --mode development && cp -r dist/* ../Server/wwwroot/",
